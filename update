. ./python_ve/bin/activate
echo 'GIT pulling.....'
git pull
echo 'PIP innstalling'
pip install -r requirements.txt
echo 'NPM installing'
npm install
echo 'Collecting static files'
./manage.py collectstatic --no-input
echo 'Migration.....'
./manage.py migrate
cd main/static
ln -s ../../node_modules node_modules

